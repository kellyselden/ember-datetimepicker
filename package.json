{
  "name": "ember-datetimepicker",
  "version": "5.0.1",
  "description": "ember addon wrapping xdan/datetimepicker",
  "keywords": [
    "ember-addon",
    "datetimepicker",
    "datepicker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kellyselden/ember-datetimepicker.git"
  },
  "license": "MIT",
  "author": "Kelly Selden",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "nodetest": "mocha node-tests --recursive"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.0",
    "broccoli-merge-trees": "^4.0.0",
    "ember-cli-babel": "^7.22.1",
    "pkg-conf": "^3.0.0"
  },
  "devDependencies": {
    "@ember/jquery": "^1.0.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@kellyselden/ember-blueprint": "2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "3.22.0",
    "ember-cli-blueprint-test-helpers": "^0.19.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-moment-shim": "^3.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.0.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "jquery-datetimepicker": "2.5.21",
    "loader.js": "^4.7.0",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "renovate-config-standard": "^2.0.0"
  },
  "peerDependencies": {
    "ember-cli-moment-shim": "",
    "jquery-datetimepicker": ""
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "classic"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://kellyselden.github.io/ember-datetimepicker"
  }
}
